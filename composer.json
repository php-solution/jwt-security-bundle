{
    "name": "php-solution/jwt-security-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for use JWT for authorization, authentication.",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Savchenko",
            "email": "dev.olsav@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/dependency-injection": ">=4.0",
        "symfony/http-kernel": ">=4.0",
        "symfony/http-foundation": ">=4.0",
        "symfony/config": ">=4.0",
        "symfony/security-core": ">=4.0",
        "symfony/security-http": ">=4.0",
        "symfony/security-guard": ">=4.0",
        "php-solution/jwt-bundle": "~0.3"
    },
    "autoload":     {
        "psr-4": { "PhpSolution\\JwtSecurityBundle\\": "src/" }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    }
}
